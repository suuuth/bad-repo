{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { io } from 'socket.io-client';\nexport default {\n  name: \"ChatBox\",\n  data() {\n    return {\n      title: 'Chat',\n      name: '',\n      text: '',\n      messages: [],\n      socket: null\n    };\n  },\n  methods: {\n    sendMessage() {\n      if (this.validateInput()) {\n        this.socket.emit('msgToServer', {\n          name: this.$store.getters.StateUser?.name,\n          text: this.text\n        });\n        this.text = '';\n      }\n    },\n    receivedMessage(message) {\n      this.messages.push(message);\n    },\n    validateInput() {\n      return this.text.length > 0;\n    }\n  },\n  created() {\n    this.socket = io(window.location.origin);\n    this.socket.on('msgToClient', message => {\n      this.receivedMessage(message);\n    });\n  }\n};","map":{"version":3,"mappings":";AAyBA,SAASA,EAAC,QAAS,kBAAiB;AACpC,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,MAAM;MACbF,IAAI,EAAE,EAAE;MACRG,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAW,GAAG;MACZ,IAAG,IAAI,CAACC,aAAa,EAAE,EAAE;QACvB,IAAI,CAACH,MAAM,CAACI,IAAI,CAAC,aAAa,EAAE;UAC9BT,IAAI,EAAE,IAAI,CAACU,MAAM,CAACC,OAAO,CAACC,SAAS,EAAEZ,IAAI;UACzCG,IAAI,EAAE,IAAI,CAACA;QACb,CAAC;QACD,IAAI,CAACA,IAAG,GAAI,EAAC;MACf;IACF,CAAC;IACDU,eAAe,CAACC,OAAO,EAAE;MACvB,IAAI,CAACV,QAAQ,CAACW,IAAI,CAACD,OAAO;IAC5B,CAAC;IACDN,aAAa,GAAG;MACd,OAAO,IAAI,CAACL,IAAI,CAACa,MAAK,GAAI;IAC5B;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACZ,MAAK,GAAIN,EAAE,CAACmB,MAAM,CAACC,QAAQ,CAACC,MAAM;IACvC,IAAI,CAACf,MAAM,CAACgB,EAAE,CAAC,aAAa,EAAGP,OAAO,IAAK;MACzC,IAAI,CAACD,eAAe,CAACC,OAAO;IAC9B,CAAC;EACH;AACF","names":["io","name","data","title","text","messages","socket","methods","sendMessage","validateInput","emit","$store","getters","StateUser","receivedMessage","message","push","length","created","window","location","origin","on"],"sourceRoot":"","sources":["/home/kevin/Projects/Personal/big-bad-project/client/src/components/ChatBox.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-box container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-md-6 offset-md-3 col-sm-12\">\r\n        <div id=\"status\"></div>\r\n        <div id=\"chat\">\r\n          <div class=\"card\">\r\n            <div id=\"messages\" class=\"card-block pt-5\">\r\n              <h3 class=\"bg-light text-left\">{{ title }}</h3>\r\n              <ul>\r\n                <li v-for=\"message of messages\" :key=\"message.id\">{{ message.name }}: {{ message.text }}</li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n          <br>\r\n          <textarea id=\"textarea\" class=\"form-control\" v-model=\"text\" placeholder=\"Enter message...\"></textarea>\r\n          <br>\r\n          <button id=\"send\" class=\"btn btn-primary\" @click.prevent=\"sendMessage\">Send</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { io } from 'socket.io-client'\r\nexport default {\r\n  name: \"ChatBox\",\r\n  data () {\r\n    return {\r\n      title: 'Chat',\r\n      name: '',\r\n      text: '',\r\n      messages: [],\r\n      socket: null\r\n    }\r\n  },\r\n  methods: {\r\n    sendMessage() {\r\n      if(this.validateInput()) {\r\n        this.socket.emit('msgToServer', {\r\n          name: this.$store.getters.StateUser?.name,\r\n          text: this.text\r\n        })\r\n        this.text = ''\r\n      }\r\n    },\r\n    receivedMessage(message) {\r\n      this.messages.push(message)\r\n    },\r\n    validateInput() {\r\n      return this.text.length > 0\r\n    }\r\n  },\r\n  created() {\r\n    this.socket = io(window.location.origin)\r\n    this.socket.on('msgToClient', (message) => {\r\n      this.receivedMessage(message)\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#messages {\r\n  height: 250px;\r\n}\r\n\r\n#messages h3 {\r\n  text-align: left;\r\n  padding: 0.5rem;\r\n  position: absolute;\r\n  top